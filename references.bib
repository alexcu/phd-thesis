arxivId = {arXiv:1606.06565v2},
author = {Amodei, Dario and Steinhardt, Jacob and Man, Dan and Christiano, Paul},
eprint = {arXiv:1606.06565v2},
file = {::},
pages = {1--29},
title = {{Concrete Problems in AI Safety}},
year = {2016}
}
